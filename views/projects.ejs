<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Project Data</title>
</head>
<body>
  <h1>Project Data</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Status</th>
        <th>Created At</th>
        <th>Updated At</th>
        <th>Deleted At</th>
      </tr>
    </thead>
    <tbody>
      <% projects.forEach(project => { %>
        <tr>
          <td><%= project.id %></td>
          <td><%= project.name %></td>
          <td><%= project.description %></td>
          <td><%= project.start_date %></td>
          <td><%= project.end_date %></td>
          <td><%= project.status %></td>
          <td><%= project.created_at %></td>
          <td><%= project.updated_at %></td>
          <td><%= project.deleted_at %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
